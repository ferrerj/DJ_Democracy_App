<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.djdemocracy"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.djdemocracy.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.djdemocracy.Home"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name="com.example.djdemocracy.ArtistList"
            android:label="@string/title_activity_artist_list" >
        </activity>
        <activity
            android:name="com.example.djdemocracy.Songs"
            android:label="@string/title_activity_songs" >
        </activity>
        <activity
            android:name="com.example.djdemocracy.Vote"
            android:label="@string/title_activity_vote" >
        </activity>
        <activity
            android:name="com.example.djdemocracy.CreateAcct"
            android:label="@string/title_activity_create_acct" >
        </activity>
        <activity
            android:name="com.example.djdemocracy.Search"
            android:label="@string/title_activity_search" >
        </activity>
        <activity
            android:name="com.example.djdemocracy.Chart"
            android:label="@string/title_activity_chart" >
        </activity>
    </application>

</manifest>
